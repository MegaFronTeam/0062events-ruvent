extends ../layout/main.pug

include ../blocks/top-nav/_top-nav.pug
include ../blocks/headerBlock/_headerBlock.pug
include ../blocks/sLogos/_sLogos.pug
include ../blocks/sWhy/_sWhy.pug
include ../blocks/sSetOfServices/_sSetOfServices.pug
include ../blocks/sOtherServices/_sOtherServices.pug
include ../blocks/sPrices/_sPrices.pug
include ../blocks/sPhotos/_sPhotos.pug 
include ../blocks/sForm/_sForm.pug 
include ../blocks/footer/_footer.pug 

block variables
	- let  mainPage= 'main-page' 
	- let  bg= 'main.jpg'

block title
	- let  title= 'Аккредитация на мероприятия, услуги по регистрации участников мероприятий | Ruvents'
	title= title
block pagecontent
	//- +mob-mnu()
	
	
	+top-nav
	main 
		+headerBlock 
		+sLogos
		+sWhy
		+sSetOfServices
		+sOtherServices
		+sPrices
		+sPhotos

		+sForm.bg-light
		+seo()


	+cookie 