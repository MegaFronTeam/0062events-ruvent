mixin sLogos(data)
	// start sLogos
	+b.sLogos#sLogos&attributes(attributes) 
		//- .container.text-secondary Нам доверяют свои мероприятия:
		+e.slider--js.swiper
			.swiper-wrapper
				mixin item(i)
					+e.slide.swiper-slide
						+e.img-wrap
							+img(`img/logo-${i}`)
				- for (var x = 1; x < 37; x++)
					if x!== 2
						if x == 9 || x == 31
							+item(x + '.svg')
						else if x == 30
							+item(x+'.jpg')
						else
							+item(x+'.png')
					
	// end sLogos