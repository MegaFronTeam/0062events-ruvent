mixin sPrices(data)
	// start sPrices
	+b.SECTION.sPrices.section#sPrices&attributes(attributes)
		.container
			+b.section-title
				h2 #[span Стоимость комплекса] услуг под ключ
			+e.table-wrap
				table
					thead
						tr
							th Количество посетителей
							th.h3.text-primary 100
							th.h3.text-primary 300
							th.h3.text-primary 500
							th.h3.text-primary 1000
					tbody
						mixin tr(t, d, count1, count2, count3, count4)
							tr
								td
									.strong= t
									if d
										.small= d
								td
									if count1
										= count1
									else
										img(src="img/svg/check.svg", alt="")
										
								td
									if count2 
										= count2
									else
										img(src="img/svg/check.svg", alt="")
								td
									if count3
										= count3
									else
										img(src="img/svg/check.svg", alt="")
								td
									if count4
										= count4
									else
										img(src="img/svg/check.svg", alt="")
						+tr('Оператор аккредитации','Время работы с 8:00 до 12:00','1','2','4','7')
						+tr('Комплект оборудования','Ноутбук + принтер','1','2','4','7')
						+tr('Печатные бейджи','105×140 мм, глянцевый или матовый, 4 + 4')
						+tr('Лента','Однотонная 16 мм, два карабина')
						+tr('Дополнительно','Сервер, роутер, сетевое оборудование, ПО')
						+tr('Стоимость','','41 000 ₽','71 000 ₽','101 000 ₽','170 000 ₽') 
			

			.row.gy-3
				.col-md-auto.order-md-last
					a.btn.btn-success(href="#" ) Запросить расчет
				.col
					p Данная стоимость указана для ориентира. Расчёт может меняться в&nbsp;зависимости от&nbsp;предоставленных вводных.
						br
						| Чтобы получить индивидуальный и&nbsp;подробный расчёт, пожалуйста, свяжитесь с&nbsp;нами любым удобным для вас способом.
	// end sPrices 